{
  "name": "@devmehq/email-validator-js",
  "version": "1.0.14",
  "private": false,
  "description": "Advanced Email Validation with DNS MX lookup and Mailbox Verification",
  "keywords": [
    "email-validation",
    "email-typos",
    "email-regex",
    "disposable-emails",
    "free-emails",
    "smtp",
    "mx-records"
  ],
  "homepage": "https://github.com/devmehq/email-validator#readme",
  "bugs": "https://github.com/devmehq/email-validator/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/devmehq/email-validator"
  },
  "license": "MIT",
  "author": "DEV.ME <support@dev.me> (https://dev.me)",
  "scripts": {
    "build": "rm -rf dist && tsc -p .",
    "lint": "eslint lib __tests__",
    "lint-fix": "yarn lint -- --fix",
    "mocha": "nyc mocha --report lcovonly -- __tests__/*-test.ts",
    "prepare": "yarn husky install",
    "prettier": "prettier --write \"src/**/*.{ts,json}\" \"__tests__/**/*.{ts,json}\"",
    "test": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha",
    "test-ci": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha"
  },
  "lint-staged": {
    "src/*.{ts,tsx,js,jsx}": "prettier --write",
    "__tests__/*.{ts,tsx,js,jsx}": "prettier --write",
    "docs/**": "prettier --write",
    "examples/**": "prettier --write"
  },
  "dependencies": {
    "@types/psl": "1.1.0",
    "psl": "1.9.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.18",
    "@types/should": "13.0.0",
    "@types/sinon": "10.0.13",
    "chai": "4.3.7",
    "eslint": "8.32.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "mocha": "10.2.0",
    "prettier": "2.8.3",
    "pretty-quick": "3.1.3",
    "should": "13.2.3",
    "sinon": "15.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">= 12.0"
  }
}
